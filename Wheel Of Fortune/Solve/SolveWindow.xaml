        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wheel_Of_Fortune.Solve"
        mc:Ignorable="d"
        Title="I'd Like To Solve The Puzzle" Height="370.774" Width="336.972" WindowStartupLocation="CenterScreen" ResizeMode="NoResize" ShowInTaskbar="False" Loaded="Window_Loaded">
    <Grid Margin="0,0,0,-3">
        <TextBox x:Name="SolveTextBox" HorizontalAlignment="Center" Height="106" TextWrapping="Wrap" VerticalAlignment="Top" Width="226" FontSize="20" Margin="33,24,33,0" BorderBrush="Black" KeyUp="SolveTextBox_KeyUp"/>

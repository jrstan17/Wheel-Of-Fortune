        <Button x:Name="SubmitButton" Content="Submit" HorizontalAlignment="Center" Height="42" VerticalAlignment="Center" Width="140" Click="SubmitButton_Click" FontSize="18" Margin="76,196,76,50"/>
        <StatusBar HorizontalAlignment="Left" Height="54" Margin="0,286,0,0" VerticalAlignment="Top" Width="329">
            <StatusBarItem RenderTransformOrigin="0.497,0.523" Width="290" FontSize="18" Margin="0,0,0,10">
                <TextBlock Name="SajakBlock" FontSize="18"/>
            </StatusBarItem>
        </StatusBar>
    </Grid>


</Window>
